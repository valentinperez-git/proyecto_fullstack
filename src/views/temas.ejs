<!-- temas.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lista de Temas</title>
</head>
<body>
  <h1>Lista de Temas</h1>
  <ul>
    <% temas.forEach(tema => { %>
      <li><a href="/foro/<%= tema._id %>"><%= tema.titulo %></a></li>
    <% }); %>
  </ul>
</body>
</html>
